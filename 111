function checkzu($parm){
    //确立数组
    $numx = array(
        1=>array("11","14","41","44"),
        2=>array("10","12","13","16","18","19","40","42","43","46","48","49"),
        3=>array("15","17","45","47"),
        4=>array("01","21","31","04","24","34"),
        5=>array("02","03","06","08","09","20","22","23","26","28","29","30","32","33","33","36","38","39"),
        6=>array("05","25","35","07","27","37")
    );
     //查询号码所在数组
    foreach ($numx as $value) {
     if(in_array($parm,$value)){
         return $parm=array_search($value,$numx);
     }
    } 
}
