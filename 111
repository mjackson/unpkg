    <script>
    	
    	$(".aaaa").click(function () {
		    $('html,body').animate({
		        scrollTop:$(".aaaaa").offset().top},{duration:500,easing:'swing'});
		    return false;
		})
		$(".shouye").click(function () {
		    $('html,body').animate({
		        scrollTop:$(".shou").offset().top},{duration:500,easing:'swing'});
		    return false;
